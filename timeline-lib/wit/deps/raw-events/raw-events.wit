package timeline:rawevents;

interface api {
  record event {
    time: u64,
    event: event-value
  }

  variant event-value{
    string-value(string),
    int-value(s64),
    float-value(f64),
    bool-value(bool),
  }

  add-event: func(order: event) -> ();

  get-event: func(time: u64) -> event;

  get-events: func() -> list<event>;
}

world rawevents {
  export api;
}
